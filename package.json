{
  "name": "arm-asl-syntax",
  "displayName": "arm-asl-syntax",
  "description": "Syntax highlighting for ARM64 Architecture Specification Language",
  "author": "Andrew Brown",
  "version": "0.0.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/andrewj-brown/arm-asl-syntax" 
  },
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "arm-asl",
      "aliases": ["ASL", "asl"],
      "extensions": [".asl"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "arm-asl",
      "scopeName": "source.asl",
      "path": "./syntaxes/asl.tmLanguage.json"
    }]
  }
}
